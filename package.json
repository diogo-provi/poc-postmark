{
  "name": "poc-postmark",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "repository": "https://github.com/diogo-provi/poc-postmark",
  "author": "Diogo Cezar",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib/",
    "build": "npm run clean && tsc",
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "dev:migrate": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/migrate.ts",
    "lint": "tsc --noEmit && eslint . && npm run clean",
    "publish": "npm publish",
    "prepublish": "npm run clean && npm run build",
    "postpublish": "npm run clean"
  },
  "dependencies": {
    "@hubspot/api-client": "^6.0.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "postmark": "^3.0.11",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.39",
    "eslint": "^8.16.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  }
}
